<!doctype html>
<html lang="fr">

    <head>
        <title>
            {% block title %}
               {{ titre }} | Mon super titre
            {% endblock %}
        </title>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        {% block css %}
            <!-- Bootstrap CSS -->
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        {% endblock %}
    </head>

    <body>

        <div class="container">
            <nav class="nav justify-content-center">
                <a class="nav-link active" href="{{ path('accueil') }}">Accueil</a>
                <a class="nav-link" href="{{ path('cgu') }}">CGU</a>
                <a class="nav-link" href="{{ path('app_login') }}">Connexion</a>
                <a class="nav-link" href="{{ path('app_logout') }}">Deconnexion</a>
                <a class="nav-link" href="{{ path('app_register') }}">s'enregistrer</a>
                <a class="nav-link" href="{{ path('utilisateur_new') }}">ajouter utilisateur</a>
                <a class="nav-link" href="{{ path('utilisateur_all') }}">Tous les utilisateurs</a>
               
  
            </nav>

            {% block body %}{% endblock %}
        </div>

        {% block js %}
            <!-- Optional JavaScript -->
            <!-- jQuery first, then Popper.js, then Bootstrap JS -->
            <script src="https://code.jquery.com/jquery-3.6.0.min.js"
        integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        {% endblock %}
    </body>
</body>
</html>